<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itheima.crm.domain.ElecRole" table="elec_role" schema="itheima1128elec">
        <id name="roleId">
            <column name="roleID" sql-type="varchar(255)"/>
        </id>
        <property name="roleName">
            <column name="roleName" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>