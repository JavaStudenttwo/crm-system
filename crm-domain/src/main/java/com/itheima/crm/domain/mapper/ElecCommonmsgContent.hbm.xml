<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itheima.crm.domain.ElecCommonmsgContent" table="elec_commonmsg_content" schema="itheima1128elec">
        <id name="comId">
            <column name="comID" sql-type="varchar(255)"/>
        </id>
        <property name="type">
            <column name="type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="orderby">
            <column name="orderby" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>