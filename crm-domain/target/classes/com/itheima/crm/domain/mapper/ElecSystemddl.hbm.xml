<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itheima.crm.domain.ElecSystemddl" table="elec_systemddl" schema="itheima1128elec">
        <id name="seqId">
            <column name="seqID" sql-type="int(11)"/>
        </id>
        <property name="keyword">
            <column name="keyword" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="ddlCode">
            <column name="ddlCode" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="ddlName">
            <column name="ddlName" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>