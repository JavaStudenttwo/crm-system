<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itheima.crm.domain.ElecText" table="elec_text" schema="itheima1128elec">
        <id name="textId">
            <column name="textID" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="textName">
            <column name="textName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="textRemark">
            <column name="textRemark" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="textDate">
            <column name="textDate" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>